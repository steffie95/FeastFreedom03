@model IEnumerable<FeastFreedom03.Models.Kitchen>

@{
    ViewBag.Title = "SelectKitchen";
    @Html.AntiForgeryToken();
}

<h4>
    Please select a kitchen <img src="http://24.media.tumblr.com/tumblr_m96h1tBM1D1qczxjoo1_400.gif" height="40" />
</h4>



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KitchenName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.KitchenName, "OrderPage", "Order", new { kitchenID = item.KitchenID }, htmlAttributes: null)
            </td>
        </tr>
    }

</table>
